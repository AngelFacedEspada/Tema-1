Algoritmo ejercicio5
	
	Definir a Como Entero;
	Definir b Como Entero;
	Definir c Como Entero;
	Escribir "Dime el numero a";
	Leer a;
	Escribir "Dime el numero b";
	Leer b;
	Escribir "Dime el numero c";
	Leer c;
	Si a < 0 Entonces
		Escribir "la multiplicación de a, b y c = ", a*b*c
	SiNo
		Escribir "la suma de a, b y c = ", a+b+c
	Fin Si
FinAlgoritmo
